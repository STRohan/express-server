{
  "compilerOptions": {
  "module": "commonjs",
  "target": "es6",
  "noImplicitAny": false,
  "moduleResolution": "node",
  "allowSyntheticDefaultImports": true,
  "resolveJsonModule": true,
  "esModuleInterop": false,
  "allowJs": true,
  "checkJs": false,
  "sourceMap": true,
  "outDir": "./dist",
  "baseUrl": ".",
  "lib": ["es2017"],
  "paths": {
  "": [
  "node_modules/",
  "src/types/"
  ]
  },
  },
  "exclude": ["dist"],
  "include": [
  "src"
  ],
  "types": ["src/types"]
  }
